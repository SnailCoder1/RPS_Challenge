{
  "name": "rps_challenge",
  "version": "1.0.0",
  "description": "Rock- Paper-Scissors Game! Rules:  Rock vs Paper -> Paper wins Rock vs Scissors -> Rock wins  Paper vs Scissor -> Scissor wins",
  "main": "index.js",
  "scripts": {
    "test": "npm run features && npm run specs",
    "features": "superstatic -p 8080 & mocha --timeout 100000 --recursive  --reporter=spec features ; PORT=8080 npm run stop-test-server",
    "specs": "mocha --recursive  --reporter=spec spec",
    "server": "superstatic src -p 3000",
    "stop-test-server": "lsof -ti tcp:$PORT | xargs kill"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SnailCoder1/RPS_Challenge.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SnailCoder1/RPS_Challenge/issues"
  },
  "homepage": "https://github.com/SnailCoder1/RPS_Challenge#readme",
  "devDependencies": {
    "e2e_training_wheels": "0.0.36"
  },
  "bin": {
    "training-wheels:generate": "node ./node_modules/e2e_training_wheels/dist/generators.js",
    "training-wheels:install": "node ./node_modules/e2e_training_wheels/dist/install.js"
  },
  "dependencies": {
    "assertion-error": "^1.1.0"
  }
}
